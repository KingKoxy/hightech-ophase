{
  "useTabs": true,
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 100,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-svelte"
  ],
  "pluginSearchDirs": [
    "."
  ],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ],
  "importOrder": [
    "^(.*)\\.svelte$",
    "^\\$lib\\/(?!assets)(.*)$",
    "^\\$lib/assets/(.*)$",
    "^(\\.\\/)?\\$houdini"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}

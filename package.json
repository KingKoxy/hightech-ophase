    "@typescript-eslint/parser": "^6.5.0",
    "contentful": "^10.5.1",
    "eslint": "8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.33.0",
    "houdini": "^1.2.9",
    "houdini-svelte": "^1.2.9",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
